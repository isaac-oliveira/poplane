<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<dexprotector>
    <verbose>true</verbose>
    <optimize>false</optimize>
    <signMode>release</signMode>
    <keystore>YOUR_KEYSTORE</keystore>
    <alias>YOUR_ALIAS</alias>
    <storepass>YOUR_STOREPASS</storepass>
    <keypass>YOUR_KEYPASS</keypass>
    <securityAssessment mode="on">
        <detectExposedSecrets mode="warning" report=""/>
        <dependencyCheck mode="warning" report=""/>
        <signingCertificateCompromised mode="error"/>
        <signingCertificateWeakKey mode="error"/>
    </securityAssessment>
    <stringEncryption mode="all"/>
    <classEncryption mode="all"/>
    <hideAccess mode="all">
        <filters>
            <filter><![CDATA[glob:!com/facebook/**]]></filter>
            <filter><![CDATA[glob:!androidx/**]]></filter>
        </filters>
    </hideAccess>
    <resourceEncryption androidManifestMangling="on" mode="all">
        <androidManifestMangling mode="all"/>
        <assets mode="all"/>
        <nameObfuscation mode="all"/>
        <strings mode="all"/>
    </resourceEncryption>
    <integrityControl mode="all">
        <checkCertificate>on</checkCertificate>
    </integrityControl>
    <antiDebug>true</antiDebug>
    <antiEmulator>true</antiEmulator>
    <antiManualInstall>false</antiManualInstall>
    <antiMalware>false</antiMalware>
    <antiXposed>true</antiXposed>
    <nativeLibraryEncryption mode="all"/>
    <runtimeChecks/>
    <uiProtection/>
</dexprotector>
